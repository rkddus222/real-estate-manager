generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Property {
  id          String   @id @default(uuid())
  title       String
  description String
  address     String
  price       Float
  area        Float
  type        String
  status      String   @default("AVAILABLE")
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
